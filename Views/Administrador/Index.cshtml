@model IEnumerable<GerenciamentoMensalidade2.Models.Administrador>

@{
    ViewData["Title"] = "Index";
}

<h1>Administrador</h1>

<p>
    <a asp-action="Create">Adicionar Administrador</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nome)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cpf)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Senha)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nome)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cpf)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Senha)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AdministradorId">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.AdministradorId">Detalhe</a> |
                <a asp-action="Delete" asp-route-id="@item.AdministradorId">Deletar</a>
            </td>
        </tr>
}
    </tbody>
</table>
